<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>


<% 
String name=(String) session.getAttribute("name"); 
%> 


<sql:query var="users" dataSource="jdbc/lut2">
    SELECT * FROM admin_users
    WHERE  uname = <%=name %>
</sql:query>

<c:set var="userDetails" value="${users.rows[0]}"/>

